USE startersql2;
SELECT * from users WHERE gender != 'Male';
SELECT * from users WHERE id<=10;
SELECT * from users WHERE date_of_birth BETWEEN '1990-09-09' AND '1999-09-09';
SELECT * from users WHERE gender in ('Male','Female');

SELECT * from users WHERE gender = 'Female' AND salary> '50000';
SELECT * from users WHERE gender = 'Male' OR salary> '50000';

SELECT * from users WHERE gender = 'Male' OR salary> '50000' ORDER BY salary DESC LIMIT 15;
